  "packageManager": "pnpm@9.14.4",
  "dependencies": {
    "@oak/oak": "npm:@jsr/oak__oak@^17.1.4"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "bumpp": "^9.10.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "wrangler": "^0.0.0-b35265a79"
  },
  "scripts": {
    "dev": "DEV=1 deno --watch --env-file=.env -A deno.ts",
